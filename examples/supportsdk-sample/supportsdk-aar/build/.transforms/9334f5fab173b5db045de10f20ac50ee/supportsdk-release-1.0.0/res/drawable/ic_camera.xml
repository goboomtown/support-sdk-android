<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="top|center_horizontal"
        android:left="0dp" android:right="0dp" android:top="0dp" android:bottom="0dp">
        <shape android:shape="oval">
            <stroke android:width="10sp"
                android:color="#56C3BF" />
            <solid android:color="#56C3BF" />
        </shape>
    </item>
    <item android:gravity="top|center_horizontal"
        android:left="2sp" android:right="2sp" android:top="2sp" android:bottom="2sp">
        <shape android:shape="oval">
            <stroke android:width="10sp"
                android:color="@android:color/white" />
            <solid android:color="@android:color/white" />
            <padding android:bottom="2sp" android:left="2sp" android:right="2sp" android:top="2sp" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_camera_alt_black_24dp" android:gravity="top|center_horizontal"
        android:left="0dp" android:right="0dp" android:top="7sp" android:bottom="0dp">
    </item>
</layer-list>
